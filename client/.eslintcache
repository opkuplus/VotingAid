 render() {\r\n      console.log(this.state.schools);\r\n      var counter = -1;\r\n      return (\r\n        <div>\r\n        {this.state.questions.map(question => {\r\n          counter++;\r\n            return (\r\n              <div className = {'set' + counter}>\r\n                  <input ref = {'question' + counter} type = \"text\" style = {{display: \"inline\",}} defaultValue = {question.question} onChange={this.handleChange.bind(this)} disabled = {this.state.disabled} />\r\n                  <select ref = {'school' + counter} /*onChange={this.handleChange.bind(this)}*/ value = {question.area} disabled = {this.state.disabled}>\r\n                    {this.state.schools.map(school => {\r\n                        return (\r\n                          <option value = {school}>{school}</option>\r\n                        );\r\n                    }\r\n                    )}\r\n                  </select>\r\n                  <IoIosBrush  name = {counter} onClick = {this.enableEdit.bind(this)}/>\r\n                  <IoIosTrash />\r\n                  <br />\r\n                  <br />\r\n              </div>\r\n            );\r\n        }\r\n      )}\r\n      <IoIosAddCircleOutline onClick = {this.addQuestion.bind(this)} style = {{cursor: 'pointer', fontSize: '50px'}} />\r\n      </div>\r\n\r\n      );\r\n    }\r\n}\r\n\r\nexport default Questions;\r\n","C:\\Users\\Omistaja\\Desktop\\VotingAid\\client\\src\\components\\registration.js",["151"],"import React, { Component } from \"react\";\r\nimport axios from \"axios\";\r\nimport { Link } from 'react-router-dom';\r\n\r\nclass Registration extends React.Component {\r\n  constructor(props) {\r\n    super(props);\r\n\r\n    this.state = {\r\n      email: \"\",\r\n      password: \"\",\r\n      password_confirmation: \"\",\r\n      registrationErrors: \"\"\r\n    };\r\n\r\n    this.handleSubmit = this.handleSubmit.bind(this);\r\n    this.handleChange = this.handleChange.bind(this);\r\n  }\r\n\r\n  handleChange(event) {\r\n    this.setState({\r\n      [event.target.name]: event.target.value\r\n    });\r\n  }\r\n\r\n  handleSubmit = event => {\r\n    event.preventDefault();\r\n\r\n    const user = {\r\n      email: this.state.email,\r\n      password: this.state.password,\r\n      password_confirmation: this.state.password_confirmation,\r\n      status: \"Candidate\"\r\n    }\r\n\r\n    axios.post('http://localhost:5000/registration',  user)\r\n      .then(res => {\r\n        console.log(user.email);\r\n        console.log(res.data);\r\n      });\r\n  }\r\n\r\n  render() {\r\n    return (\r\n      <div>\r\n        <form onSubmit={this.handleSubmit}>\r\n          <input type=\"email\" name=\"email\" placeholder=\"Email\" value={this.state.email} onChange={this.handleChange} required/>\r\n          <input type=\"password\" name=\"password\" placeholder=\"Password\" value={this.state.password} onChange={this.handleChange} required/>\r\n          <input type=\"password\" name=\"password_confirmation\" placeholder=\"Password confirmation\" value={this.state.password_confirmation} onChange={this.handleChange} required/>\r\n          <button type=\"submit\">Register</button>\r\n          <Link\r\n          to={{\r\n            pathname: \"/\"\r\n          }}> Back to MainPage </Link>\r\n        </form>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default Registration;\r\n","C:\\Users\\Omistaja\\Desktop\\VotingAid\\client\\src\\components\\candidates.js",["152","153"],"import React, { Component } from 'react';\r\nimport { UserContext } from '../context/userContext';\r\nimport Table from 'react-bootstrap/Table';\r\nimport axios from \"axios\";\r\nimport { Link } from 'react-router-dom';\r\nimport Profile from './profile.js';\r\n\r\n  class Candidates extends Component {\r\n    constructor(props) {\r\n      super(props);\r\n      this.state = {\r\n        amount: [],\r\n        schools: [],\r\n      }\r\n    }\r\n\r\n    componentDidMount() {\r\n     axios.get('http://localhost:5000/')\r\n       .then(res => {\r\n         let q = [];\r\n         let s = [];\r\n         for(var i = 0; i < res.data.length;i++) {\r\n           this.setState({ ['Candidate' + i]: res.data[i]})\r\n           q.push(res.data[i].name);\r\n           var joined = this.state.amount.concat(q[i]);\r\n           this.setState({ amount: joined });\r\n           s.push(res.data[i].school);\r\n\r\n         }\r\n         const uniqueSchools = Array.from(new Set(s));\r\n         this.setState({ schools: uniqueSchools });\r\n     });\r\n   }\r\n\r\n   handleChange() {\r\n     axios.post('http://localhost:5000/filteredCandidates',{data: this.refs.school.value})\r\n       .then(res => {\r\n         let q = [];\r\n         for(var i = 0; i < res.data.length;i++) {\r\n           this.setState({ ['Candidate' + i]: res.data[i]})\r\n           q.push(res.data[i].name);\r\n           this.setState({ amount: q});\r\n         }\r\n       });\r\n   }\r\n\r\n\r\n    render() {\r\n      var counter = -1;\r\n      return (\r\n        <div style = {{width: '50%', marginLeft: '25%', marginTop: '5%'}}>\r\n        <label htmlFor =\"school\">Filter by School</label>\r\n        <select ref=\"school\" onChange={this.handleChange.bind(this)}>\r\n          {this.state.schools.map(index => {\r\n              return (\r\n                <option value = {index}>{index}</option>\r\n              );\r\n          }\r\n          )}\r\n        </select>\r\n          <Table striped bordered hover variant=\"dark\">\r\n            <thead>\r\n              <tr>\r\n                <th>#</th>\r\n                <th>Candidate</th>\r\n                <th>School</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n            {this.state.amount.map(index => {\r\n              counter++;\r\n                return (\r\n                    <tr>\r\n                      <td>{counter + 1}</td>\r\n                      <td style = {{cursor: \"pointer\"}}>\r\n                      <Link\r\n          